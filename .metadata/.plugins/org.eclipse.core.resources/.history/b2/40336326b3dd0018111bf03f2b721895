
public class CharTypeUnicode {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char ch1 = 'Çæ';
		char ch2 = 'È®';
		System.out.println(ch1 + " " + ch2);
		
		char ch3 = 0xD5D0;
		char ch4 = 0xD655;
		System.out.println(ch3 + " "  + ch4);
		
		char ch5 = 54736;
		char ch6 = 54869;
		System.out.println(ch5 + " "  + ch6);
	}

}
