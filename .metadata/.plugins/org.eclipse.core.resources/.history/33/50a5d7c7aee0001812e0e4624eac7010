package com.biz.var;

public class Variable07 {
	public static void main(String[] args) {
		// 선행 연산자 : 변수명 앞에 ++, -- 연산자가 먼저 오는경우
		int intNum1 = 0;
		
		++intNum1; // intNum1을 1 증가시켜라
		intNum1 += 1;
		intNum1 = intNum1 + 1;
		
		--intNum1; // intNum1을 1 감소시켜라
		intNum1 -= 1;
		intNum1 = intNum1 - 1;
		
		// 후행 연산자 : 변수명 뒤에 ++, -- 연산자가 나중에 오는 경우
		intNum1++;
		intNum1 += 1;
		intNum1 = intNum1 + 1;
		
		intNum1--;
		intNum1 -= 1;
		intNum1 = intNum1 - 1;
	}
}
